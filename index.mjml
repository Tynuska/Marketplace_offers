                  <!--[if mso]>
                    </center>
                  </v:roundrect>
                <![endif]-->
                </div>
              </mj-raw>
            </mj-text>
          </mj-column>
        </mj-section>

        <!-- footer -->
        <mj-section mj-class="section" padding-top="34px" padding-bottom="20px" background-color="#4A4A4A" css-class="padding-mobile">
          <mj-column padding-left="20px" padding-left="20px">
            <mj-text color="#FFF" font-size="14px" line-height="120%" font-family="Skoda Next, Arial" padding-bottom="15px">
              <b>ŠKODA AUTO a.s.</b><br />
              Tř. Václava Klementa 869<br />
              293 01 Mladá Boleslav II<br />
              Czech Republic<br />
              <a href="https://www.skoda-auto.com/" target="_blank" style="text-decoration: none"><span style="color: #FFF; text-decoration: none" target="_blank">www.skoda-auto.com</span></a>
            </mj-text>
          </mj-column>

          <mj-column vertical-align="top" padding-right="20px">
            <mj-raw>
              <table style="width: 100%">
                <tr style="vertical-align: top;">
                  <td style="text-align: right; text-decoration: none" text-docoration="none">
